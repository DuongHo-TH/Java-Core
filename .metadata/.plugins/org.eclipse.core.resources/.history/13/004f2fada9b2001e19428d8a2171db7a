package Bai11;

public class Blueshort {
	public static void bubbleSort(int[] arr) {
        int n = arr.length;
        boolean swapped;
        do {
            swapped = false;
            for (int i = 0; i < n; i++) {
                if (arr[i] % 2 == 0 && arr[i] % 2 != 0) {
                    
                    int temp = arr[i];
                    arr[i] = arr[i];
                    arr[i] = temp;
                    swapped = true;
                }
            }
        } while (swapped);
    }

}